/* Hoja de estilo index */
h1{
	 color: blue;
}
body{
    background-size: cover;
}
a:link {
    /* Enlaces no visitados */
    color: rgb(38, 0, 255);
}

a:visited {
    /* Enlaces visitados */
    color: rgb(38, 0, 255);
}

a:hover,
:focus {
    color: rgb(97, 110, 146);
    /* El usuario esta sobre el enlace */
}

a:active {
    color: rgb(58, 46, 163);
    /* El usuario hace click en el enlace */
}